---
import '../styles/global.css';
import '../js/parallax.js';

import Layout from "../layouts/Layout.astro";
import { Home } from "../components/Home/Home.jsx";
import { Over } from "../components/Over/Over.jsx";
import { Expertise } from "../components/Expertise/Expertise.jsx";
import { Klimaatadaptatie } from "../components/Klimaatadaptatie/Klimaatadaptatie.jsx";
import { Waterkwaliteit } from "../components/Waterkwaliteit/Waterkwaliteit.jsx";
import { Brain } from "../components/Brain/Brain.jsx";
import { Projecten } from "../components/Projecten/Projecten.jsx";
import { Kennisbank } from "../components/Kennisbank/Kennisbank.jsx";
import { Team } from "../components/Team/Team.jsx";
import { Contact } from "../components/Contact/Contact.jsx";
import { Partners } from "../components/Partners/Partners.jsx";

import { fetchData } from "../components/Kennisbank/Kennisbank.jsx";

let data;

(async () => {
  try {
    ({ data } = await fetchData()); // assign the value to the outer data variable
    console.log(data);
  } catch (error) {
    console.error('Error:', error);
  }
})();

---

<Layout title="homepage">
	<Home />
	<Over />
	<Expertise />
	<Klimaatadaptatie />
	<Waterkwaliteit />
	<Brain />
	<Projecten />
	{data && <Kennisbank data={data}/>}
	<Team />
	<Contact />
	<Partners />
</Layout>

